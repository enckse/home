[commit]
    gpgsign = true
[core]
    editor = vim
    autocrlf = input
[init]
    templateDir = ~/.local/git
[filter "lfs"]
    process = git-lfs filter-process
    required = true
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
[grep]
    lineNumber = true
[include]
    path = ~/store/personal/config/etc/gitconfig
[log]
    date = iso-strict
[pager]
    log = /usr/share/git/diff-highlight/diff-highlight | less
    show = /usr/share/git/diff-highlight/diff-highlight | less
    diff = /usr/share/git/diff-highlight/diff-highlight | less
[pull]
    rebase = false
[push]
    default = simple
[sendemail]
    smtpserver = "/usr/bin/msmtp"
    smtpencryption = tls
[status]
    short = true
    branch = true
    showStash = true
    showUntrackedFiles = all
[tag]
    gpgSign = true
[user]
    name = Sean Enck
    email = enckse@voidedtech.com
    signingKey = 031E9E4B09CFD8D3F0ED35025109CDF607B5BB04
[help]
    format = man
