[core]
    editor = vim
    autocrlf = input
[grep]
    lineNumber=true
[push]
    default = simple
[user]
    name = Sean Enck
    email = enckse@voidedtech.com
    signingKey = 031E9E4B09CFD8D3F0ED35025109CDF607B5BB04
[commit]
    gpgsign = true
[sendemail]
    smtpserver = "/usr/bin/msmtp"
    smtpencryption = tls
[include]
    path = ~/store/personal/config/etc/gitconfig
[filter "lfs"]
    process = git-lfs filter-process
    required = true
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
[pager]
    log = /usr/share/git/diff-highlight/diff-highlight | less
    show = /usr/share/git/diff-highlight/diff-highlight | less
    diff = /usr/share/git/diff-highlight/diff-highlight | less
[pull]
	rebase = false
[log]
	date = iso-strict
[status]
	short = true
	branch = true
	showStash = true
[tag]
	gpgSign = true
